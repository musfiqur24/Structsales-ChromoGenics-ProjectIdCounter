{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/", "dest": "/api/index" },
    { "src": "/api/(.*)", "dest": "/api/$1" }
  ],
  "env": {
    "MONGO_URI": "@mongo_uri",
    "HUBSPOT_CLIENT_ID": "@hubspot_client_id",
    "HUBSPOT_CLIENT_SECRET": "@hubspot_client_secret",
    "HUBSPOT_REDIRECT_URI": "@hubspot_redirect_uri",
    "START_VALUE": "@start_value"
  }
}